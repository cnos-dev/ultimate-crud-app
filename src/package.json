{
  "name": "ultimate-crud-simple",
  "version": "1.0.0",
  "description": "Simple blog application demonstrating Ultimate CRUD package",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "start:sqlite": "DB_DIALECT=sqlite node index.js",
    "start:mysql": "DB_DIALECT=mysql node index.js",
    "start:postgres": "DB_DIALECT=postgres node index.js",
    "dev": "nodemon index.js",
    "dev:sqlite": "DB_DIALECT=sqlite nodemon index.js",
    "dev:mysql": "DB_DIALECT=mysql nodemon index.js",
    "dev:postgres": "DB_DIALECT=postgres nodemon index.js",
    "setup": "./setup-db.sh",
    "setup:sqlite": "./setup-db.sh sqlite",
    "setup:mysql": "./setup-db.sh mysql",
    "setup:postgres": "./setup-db.sh postgres",
    "setup:both": "./setup-db.sh both",
    "db:stop": "./setup-db.sh stop",
    "db:reset": "./setup-db.sh reset",
    "db:status": "./setup-db.sh status",
    "db:logs": "./setup-db.sh logs",
    "test": "echo \"Run setup first, then npm start\""
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mysql2": "^3.6.0",
    "pg": "^8.11.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.0",
    "sqlite3": "^5.1.6",
    "ultimate-crud": "^1.0.0-alpha.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "ultimate-crud",
    "crud",
    "api",
    "blog",
    "simple",
    "rest",
    "graphql"
  ],
